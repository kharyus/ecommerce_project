<table>
  <tr>
    <th>Order ID</th>
    <th>Total</th>
    <th>Status</th>
  </tr>

    <% @orders.each do |order| %>
        <tr>
            <td><%= link_to "#{order.id}", "/order/#{order.id}" %></td>
            <td><%= order.total %></td>
            <td><%= order.status %></td>
        </tr>
    <% end %>

</table>
